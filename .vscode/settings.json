{
  /* Terminal -> */
  "cSpell.language": "en,pt,en-US,en-GB,lorem,pt-BR",
  "explorer.compactFolders": false,
  "screencastMode.keyboardShortcutsFormat": "keys",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.enableBell": true,
  "terminal.integrated.profiles.windows": {
    "cmder": {
      "path": "C:\\WINDOWS\\System32\\cmd.exe",
      "args": [
        "/K",
        "C:\\Users\\root\\Apps\\Cmder\\vendor\\bin\\vscode_init.cmd"
      ]
    }
  },
  "terminal.integrated.defaultProfile.windows": "cmder",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  /* <- Terminal */
  "editor.autoIndent": "full",
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnType": true,
  "editor.tabCompletion": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.vscode": false,
    "node_modules": true,
    "**lock*": true
  },
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Default Dark+",
  "tabnine.experimentalAutoImports": true,
  "window.zoomLevel": 0,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "yarn",
  "parameterHints.languages": [
    "all"
  ],
  /* TEST CONFIG -> */
  "test-generator.sourceRoot": "./src", // Sets the file root source to './src'
  "test-generator.testDirectory": "./", // Outputs the test file in "__tests__/index.test.tsx",
  "test-generator.fileSuffix": "test", // Outputs the file to "__tests__/index.test.tsx"
  "test-generator.noTestDirectory": true, // Outputs the file to "./src/index.test.tsx",
  "test-generator.testSnippet": [
    "describe('${1:Component}', () => {",
    " test('if should be generated ', ()=> {",
    " | ",
    "  }) ",
    "})"
  ],
  "testing.automaticallyOpenPeekView": "never",
  "explorerExclude.backup": {},
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "cSpell.words": [
    "postgres"
  ] // Renders a snippet in the newly created test file. see https://code.visualstudio.com/docs/editor/userdefinedsnippets for documentation on UserSnippets
  /* <- TEST CONFIG */
}